<% # encoding: utf-8 %>
<h1>客户列表</h1>

<% if @contact_lists.blank? %>
    <p>没有搜到相同手机号码的联系人</p>
<% else %>
    <table width="100%" class="khsstanchu" bgcolor="#E1FFB9">
      <tr>
        <th>客户名称</th>
        <th>联系人姓名</th>
        <th>手机号码</th>
        <th></th>
      </tr>
        <% @contact_lists.each_with_index do |contact_list,num| %>
          <tr>
            <td><%= contact_list.client_name %></td>
            <td><%= contact_list.name %></td>
            <td><%= contact_list.mobile %></td>
            <td></td>
        <% end %>
    </table>
<% end %>
